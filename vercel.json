{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/((?!api).*)",
            "destination": "/"
        }
    ],
    "functions": {
        "api/webhook.ts": {
            "memory": 256,
            "maxDuration": 10
        }
    }
}